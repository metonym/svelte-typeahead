{
  "name": "svelte-typeahead",
  "version": "5.0.0",
  "license": "MIT",
  "description": "Accessible, fuzzy search typeahead component",
  "author": "Eric Liu (https://github.com/metonym)",
  "type": "module",
  "svelte": "./src/index.js",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./*.svelte": {
      "types": "./src/*.svelte.d.ts",
      "import": "./src/*.svelte"
    },
    "./*.js": {
      "types": "./src/*.d.ts",
      "import": "./src/*.js"
    },
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "svelte": "./src/index.js"
    }
  },
  "scripts": {
    "dev": "rollup -cw",
    "test": "svelte-check --workspace tests"
  },
  "dependencies": {
    "fuzzy": "0.1.3",
    "svelte-search": "^2.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@types/bun": "^1.1.17",
    "culls": "^0.1.1",
    "svelte": "^3.59.1",
    "svelte-check": "^4.1.4",
    "svelte-readme": "^3.6.3",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metonym/svelte-typeahead.git"
  },
  "homepage": "https://github.com/metonym/svelte-typeahead",
  "bugs": "https://github.com/metonym/svelte-typeahead/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "typeahead",
    "fuzzy",
    "highlight",
    "search",
    "filter",
    "WAI-ARIA",
    "accessibility"
  ],
  "files": [
    "src"
  ]
}
